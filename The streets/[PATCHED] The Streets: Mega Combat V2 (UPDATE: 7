mouse.KeyDown:Connect(function(key)
    if key == "r" and rolldebounce == false then
        rolldebounce = true
        local Anim = Instance.new("Animation")
        Anim.AnimationId = "rbxassetid://229782914"
        local roll = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(Anim)
        roll.Name = "FUCK"
        if game.Players.LocalPlayer.Character ~= nil then
            if game.Players.LocalPlayer.Character:FindFirstChild("Left Leg") and game.Players.LocalPlayer.Character:FindFirstChild("Right Leg") then
                game.Players.LocalPlayer.Character:FindFirstChild("Left Leg").Parent = game.Workspace.Terrain
                game.Players.LocalPlayer.Character:FindFirstChild("Right Leg").Parent = game.Workspace.Terrain
            end
        end
        roll:Play(0)
        roll:AdjustSpeed(1)
        roll.Stopped:Connect(function()
            if game.Workspace.Terrain:FindFirstChild("Left Leg") and game.Workspace.Terrain:FindFirstChild("Right Leg") and game.Players.LocalPlayer.Character ~= nil then
                game.Workspace.Terrain:FindFirstChild("Left Leg").Parent = game.Players.LocalPlayer.Character
                game.Workspace.Terrain:FindFirstChild("Right Leg").Parent = game.Players.LocalPlayer.Character
            end
            rolldebounce = false
        end)
        repeat 
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-1)
            wait(0.000001)
        until rolldebounce == false
    end
end)

