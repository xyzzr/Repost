            if (v:IsA("Tool") and v.Name == "BoomBox") then
                v.Parent = game.Players.LocalPlayer.Character
            end
        end
        wait(0.1)
        for _,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
            if (v:IsA("Tool") and v.Name == "BoomBox") then
            v.Remote:FireServer("PlaySong", idtoplay)
            end
        end
    else
        promptgod()
    end
end

